command /clearparcelle <text>:
	trigger:
		if player is {staff.service::%player%}:
			clear {parcelle_n%arg 1%.proprio}
			broadcast "&e&l>> &bLa Parcelle &eN°1 &bn'a plus de propriétaire !"
			if {parcelle_n%arg 1%.InReset} is set:
				send "&5[Reset] &cReset en cours !"
				stop
			set {parcelle_n%arg 1%.InReset} to true
			send "&5[Reset] &aIl y a &c%{parcelle_n%arg 1%.Count}% blocks &aà modifier !"
			set {_parcelle_n%arg 1%.count} to 0
			loop {parcelle_n%arg 1%.Count} times:
				clear {parcelle_n%arg 1%.blockis.%{parcelle_n%arg 1%.blocklocation.%{_parcelle_n%arg 1%.count}%}%} 
				set block at {parcelle_n%arg 1%.blocklocation.%{_parcelle_n%arg 1%.count}%} to {parcelle_n%arg 1%.block.%{_parcelle_n%arg 1%.count}%}
				clear {parcelle_n%arg 1%.blocklocation.%{_parcelle_n%arg 1%.count}%}
				clear {parcelle_n%arg 1%.block.%{_parcelle_n%arg 1%.count}%}
				send "Reset de map : %{_parcelle_n%arg 1%.count}% / %{parcelle_n%arg 1%.Count}%" to console
				add 1 to {_parcelle_n%arg 1%.count}
				wait 1 tick
			set {parcelle_n%arg 1%.Count} to 0
			clear {parcelle_n%arg 1%.InReset}
			send "&5[Reset] &aReset fini &c(%{_parcelle_n%arg 1%.count}%)"	

				
