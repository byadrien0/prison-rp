command /reset <text>:
	trigger:
		if arg 1 is "parcelle_n1":
			if player is "%{parcelle_n1.proprio}%":
				if {parcelle_n1.InReset} is set:
					send "&5[Reset] &cReset en cours !"
					stop
				set {parcelle_n1.InReset} to true
				send "&5[Reset] &aIl y a &c%{parcelle_n1.Count}% blocks &aà modifier !"
				set {_parcelle_n1.count} to 0
				loop {parcelle_n1.Count} times:
					clear {parcelle_n1.blockis.%{parcelle_n1.blocklocation.%{_parcelle_n1.count}%}%} 
					set block at {parcelle_n1.blocklocation.%{_parcelle_n1.count}%} to {parcelle_n1.block.%{_parcelle_n1.count}%}
					clear {parcelle_n1.blocklocation.%{_parcelle_n1.count}%}
					clear {parcelle_n1.block.%{_parcelle_n1.count}%}
					send "Reset de map : %{_parcelle_n1.count}% / %{parcelle_n1.Count}%" to console
					add 1 to {_parcelle_n1.count}
					wait 1 tick
				set {parcelle_n1.Count} to 0
				clear {parcelle_n1.InReset}
				send "&5[Reset] &aReset fini &c(%{_parcelle_n1.count}%)"
				
